{
  "name": "upvotebay",
  "version": "0.0.0",
  "homepage": "https://github.com/strayduy/upvotebay",
  "authors": [
    "Duy Tran <duy@strayharbor.com>"
  ],
  "license": "MIT",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "angular": "~1.2.3",
    "bootstrap": "~3.0.2",
    "jquery": "~2.0.3"
  },
  "exportsOverride": {
      "angular": {
          "css": "*.css",
          "js": ["*.js", "*.js.map"]
      },
      "bootstrap": {
          "js": "dist/js/*.js",
          "css": "dist/css/*.css",
          "font": "dist/fonts/*"
      },
      "jquery": {
          "js": "*.js"
      }
  }
}
